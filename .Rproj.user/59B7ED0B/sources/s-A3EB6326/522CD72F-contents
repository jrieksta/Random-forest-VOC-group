#if the herbivory effects were ceased after the removal of larvae ################
d1_post_herb <-d1 %>% 
  filter(Date=="July 11")
 #ph = post herbivory

GLV_ph<-subset(d1_post_herb,(Compound_group=="green leaf volatile")) 
HT_ph<-subset(d1_post_herb,(Compound_group=="HT")) 
HC_ph<-subset(d1_post_herb,(Compound_group=="hydrocarbons"))
ISO_ph<-subset(d1_post_herb,(Compound_group=="isoprenoids"))
MT_ph<-subset(d1_post_herb,(Compound_group=="MT")) 
other_ph<-subset(d1_post_herb,(Compound_group=="other"))
OVOC_ph<-subset(d1_post_herb,(Compound_group=="oxygenated VOCs"))
SQT_ph<-subset(d1_post_herb,(Compound_group=="SQT")) 
total_ph<-subset(d1_post_herb,(Compound_group=="Total"))

total$logconc <- log(total$sum+1) 
MT$logconc <- log(MT$sum+1) 


test<-ddply(total,.(treatment),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )
test<-ddply(total,.(Date),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )
test<-ddply(total,.(herbivory),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )
test<-ddply(MT,.(herbivory,Date),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )
test<-ddply(SQT,.(herbivory),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )

test<-ddply(MT,.(treatment),summarize, N=length(sum), mean=mean(sum), sd=sd(sum),se=sd/sqrt(N) )

