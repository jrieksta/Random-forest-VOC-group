#leaf damage was assessed at the end of the campaign. 
names(d1)
leaf_damage<-ddply(d1,.(trtmt, treatment, herbivory, Plot, Block),summarize, N=length(damaged.leaves), mean=mean(damaged.leaves), sd=sd(damaged.leaves),se=sd/sqrt(N) )

leaf_damage2<-ddply(d1,.(treatment,herbivory),summarize, N=length(damaged.leaves), mean=mean(damaged.leaves), sd=sd(damaged.leaves),se=sd/sqrt(N) )

leaf_damag1<-ddply(d1,.(herbivory),summarize, N=length(damaged.leaves), mean=mean(damaged.leaves), sd=sd(damaged.leaves),se=sd/sqrt(N) )

names(d1)
m0<-lmer(mean~treatment*herbivory+(1|Block/Plot),data=leaf_damage)
m1=lme(mean~treatment*herbivory, random=~1|Block/Plot, method="ML", data=leaf_damage)
anova(m1)
summary(m1)

m0<-lmer(mean~treatment*herbivory+(1|Block/Plot),data=leaf_damage)

plot(DD(mean~treatment*herbivory,random=~Block/Plot,data=leaf_damage))

drop1(m0,test="Chisq")
tab_model(m0)
anova(m0)
summary(m0)
Model.pval<-pvals.fnc(m0, nsim = n, withMCMC = TRUE)



plot(m0)
qqnorm(residuals(m0))
plot()
r.rmse <- sqrt(mean(residuals(m0)^2))
plot(fitted(m0),leaf_damage$mean)

library(lmerTest)


#treatment
emmeans(m0, specs=trt.vs.ctrl~treatment )
emmeans(m1, specs=trt.vs.ctrl~treatment )

emmeans(m0, specs=trt.vs.ctrl~Date|herbivory)
emmeans(m0, specs=trt.vs.ctrl~Date|herbivory)


#emmeans herbivory|Treatment
y=emmeans(m0,~herbivory|treatment, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05)
y=emmeans(m1,~herbivory|treatment, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05)



