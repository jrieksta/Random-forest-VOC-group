Date_MT <- emmeans(m3,~Date, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05, bias.adj=TRUE, sigma=0.5872692)
Date_MT
#herbivory
emmeans(m4, specs=trt.vs.ctrl~herbivory )
emmeans(m3,~herbivory, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05, bias.adj=TRUE, sigma=0.5872692)

lsmeansherb<- emmeans(m3,~herbivory|Date, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05, bias.adj=TRUE, sigma=0.5872692)
lsmeansherb

lsmeansherb<- emmeans(m3,~herbivory|treatment|Date, cont="pairwise", int.adjust="tukey", type="response", alpha=0.05, bias.adj=TRUE, sigma=0.5872692)
lsmeansherb
x <- as.data.frame(lsmeansherb$emmeans)
x$Date1 = factor(x$Date, levels=c('June 30','July 5'))

