  geom_bar(stat="identity", color="black", 
           position=position_dodge()) +
  facet_wrap(Date1 ~ treatment, ncol=9, nrow=6)+ggtitle("MTs")+
  theme_bw() + labs(y="emmean", x="Treatment") +
